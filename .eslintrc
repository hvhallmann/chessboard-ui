{
  "extends": [
    "react-app",
    "airbnb-base",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "import/no-cycle": [
      {
        "maxDepth": "Infinity"
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "jsx-a11y/label-has-for": 0
  },
  "plugins": [
    "jsx-a11y"
  ],
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  }
}
